{
    "dev": {
        "ids": "debian",
        "name": "debian",
        "sw_version": "Debian GNU/Linux 12 (bookworm)",
        "model": "AMD Ryzen 5 8600G w/ Radeon 760M Graphics"
    },
    "o": {
        "name": "linux_mqtt_ha",
        "sw": "1.1",
        "url": "https://github.com/susembed/linux_mqtt_ha"
