            "p": "sensor",
            "name": "Last Boot",
            "icon": "mdi:clock",
            "state_topic": "homeassistant/linux_ha_mqtt_debian/one_time",
            "value_template": "{{ (now() | as_timestamp - (value_json.uptime |float(0))) |round(0) | as_datetime |as_local}}",
            "device_class": "timestamp",
            "unique_id": "debian_last_boot"
        },
